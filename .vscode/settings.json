{
    "json.schemas": [
        {
            "fileMatch": [
                "/lang/*.json"
            ],
            "url": "./.vscode/probe.lang-schema.json"
        },
        {
            "fileMatch": [
                "/probe/docs/*.json"
            ],
            "url": "./.vscode/probe.doc-schema.json"
        }
    ],
    "files.exclude": {
        "config/*.bak": true,
        "config/sodium-fingerprint.json": true,
        "config/sodium-options.json": true,
        "**/heracles_options.json": true,
        "**/icon.png": true,
        "**/irisUpdateInfo.json": true,
        "**/options.txt": true,
        "**/patchoulie_data.json": true,
        "**/polymer-resourcepack.zip": true,
        "**/rhino.local.properties": true,
        "**/usercache.json": true,
        "**/usernamecache.json": true,
        "**/.fabric/": true,
        "**/.mixin.out/": true,
        "**/coremods/": true,
        "**/crash-reports/": true,
        "**/data/": true,
        "**/local/": true,
        "**/logs/": true,
        "**/moddata/": true,
        "**/modernfix/": true,
        "**/ModTranslations/": true,
        "**/patchouli_books/": true,
        "**/saves/": true,
        "**/schematics/": true,
        "**/screenshots/": true,
        "**/server-resource-packs/": true,
        "**/shaderpacks/": true,
        "**/texturepacks/": true,
        "kubejs/probe/**/*": true,
        "kubejs/probe/**/*/": true,
        "**/knownkeys.txt": true,
        "**/patchouli_data.json": true,
        "**/Xaero*/": true,
        "**/xaero*/": true,
        ".vscode/probe*": true,
        ".vscode/fluid-attributes.json": true,
        ".vscode/item-attributes.json": true,
        ".vscode/item-tag-attributes.json": true,
        ".vscode/lang-keys.json": true,
        "mods/.index": true
    }
}